{
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/curso/(.*)",
        "dest": "./index.js"
      },
      {
        "src": "/api/(.*)",
        "dest": "./index.js"
      },
      {
        "src": "/(.*)",
        "dest": "./index.js"
      }
    ]
  }
  